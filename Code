import random

file=open("dataoutput.txt","r+")
file2=open("Pre_topic_output.txt","r+")

Topic_data=[]
data = []
mean_array=[]
Sax_data=[]
Mean_array_finsih=[]
The_likely_Defuzzy=[]
SD = 0 #Standard Deviation

for i in range(100):
    data.append(random.randint(0,100))

for word in file.read().split():
    Sax_data.append(int(word))

for Topic in file2.read().split():
    Topic_data.append(round(float(Topic)))


data.sort()
counter=0
interval = len(data)/4

for i in range(4):
    temp_sum = 0
    for j in range(interval):
        temp_sum += float(data[counter])
        counter +=1
    temp_aver = float(temp_sum/interval)
    globals()['mean%s' % (i)] = temp_aver
    mean_array.append(globals()['mean%s' % (i)])

for i in range(len(Sax_data)):
    temp_data=Sax_data[i]
    n = 0
    TerVar1 = 1
    temp_mean = 0
    while TerVar1 != 0:
        Temp_slice_data = int(temp_data%10)
        temp_mean += float(mean_array[Temp_slice_data - 1])
        temp_data = int (temp_data/10)
        TerVar1 = temp_data
        n = n + 1
    #print temp_mean
    Mean_array_finsih.append(temp_mean/(n))

print mean_array
print Mean_array_finsih

for i in range(len(Topic_data)):
    temp_int = int(Topic_data[i])
    The_likely_Defuzzy.append(mean_array[temp_int-1])

print The_likely_Defuzzy

